{
  "name": "Rust",
  "build": {
    "dockerfile": "Dockerfile",
    "cacheFrom": "ghcr.io/nikobockerman/adventofcode-rust-devcontainer:cache"
  },
  "customizations": {
    "vscode": {
      "extensions": ["rust-lang.rust-analyzer"],
      "settings": {
        "[rust]": {
          "editor.defaultFormatter": "rust-lang.rust-analyzer"
        }
      }
    }
  }
}
